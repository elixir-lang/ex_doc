<%= head_template(config, %{title: "Table Of Contents"}) %>
    <h1>Table of contents</h1>
    <nav epub:type="toc">
      <ol>
        <%= for extra <- config.extras do %>
          <li><a href="<%= extra_title(extra) %>.xhtml"><%= extra_title(extra) %></a></li>
        <% end %>
        <%= for node <- nodes do %>
        <li><a href="<%= node.id %>.xhtml"><%= node.id %></a></li>
        <% end %>
      </ol>
    </nav>
  </body>
</html>
