<%= head_template(config, %{title: module.id <> ".#{name}", type: module.type}) %>
<%= sidebar_template(config, nodes_map) %>

<h1><%= module.title <> ".#{name}" %></h1>

<div class="deprecated">
  You're seeing just the <%= type %> <code><%= name %></code>, go back to <a href="<%= module.id %>.html"><%= module.title %></a> module for more information.
</div>

<%= for node <- nodes do %>
  <%= detail_template(node, module) %>
<% end %>

<%= footer_template(config) %>
