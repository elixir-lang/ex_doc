    <%= head_template(config, %{id: :overview}) %>
    <%= sidebar_template(config, modules, exceptions, protocols, has_readme) %>

      <div class="breadcrumbs">
        <button type="button" class="btn btn-default btn-sm" data-toggle="offcanvas" aria-label="Close">
          <span class="fa fa-bars" aria-hidden="true"></span>
        </button>
        <%= if has_readme do %>
          <%= page_breadcrumbs(config, "Overview", "overview.html#content") %>
        <% end %>
      </div>

      <h1><%= config.project %> v<%= config.version %></h1>

      <ul class="summary_links">
        <%= unless Enum.empty?(modules) do %>
          <li><a href="#modules_summary">Modules</a></li>
        <% end %>
        <%= unless Enum.empty?(exceptions) do %>
          <li><a href="#exceptions_summary">Exceptions</a></li>
        <% end %>
        <%= unless Enum.empty?(protocols) do %>
          <li><a href="#protocols_summary">Protocols</a></li>
        <% end %>
      </ul>

      <%= unless Enum.empty?(modules) do %>
        <h1 id="modules_summary" class="section_heading">Modules summary <%= to_top_link() %></h1>
        <div class="summary">
          <%= for node <- modules, do: overview_entry_template(node) %>
        </div>
      <% end %>

      <%= unless Enum.empty?(exceptions) do %>
        <h1 id="exceptions_summary" class="section_heading">Exceptions summary <%= to_top_link() %></h1>
        <div class="summary">
          <%= for node <- exceptions, do: overview_entry_template(node) %>
        </div>
      <% end %>

      <%= unless Enum.empty?(protocols) do %>
        <h1 id="protocols_summary" class="section_heading">Protocols summary <%= to_top_link() %></h1>
        <div class="summary">
          <%= for node <- protocols, do: overview_entry_template(node) %>
        </div>
      <% end %>
    <%= footer_template %>
